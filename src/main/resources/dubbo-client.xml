    <dubbo:protocol name="dubbo" port="20880"/>

    <dubbo:reference id="helloService" interface="com.hjx.dubbo.interfaces.APIHello"
    url="dubbo://192.168.127.1:20880/com.hjx.dubbo.interfaces.APIHello"/>

</beans>